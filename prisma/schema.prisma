datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model WebsiteVisit {
  id        Int      @id
  total     BigInt   @default(0)
  daily     Int      @default(0)
  weekly    Int      @default(0)
  monthly   Int      @default(0)
  yearly    Int      @default(0)
  lastHit   DateTime @default(now())
}
